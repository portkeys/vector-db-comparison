# Vector Database API Keys and Configuration
# Copy this file to .env and fill in your actual credentials

# Chroma Cloud
CHROMA_API_KEY=your_chroma_api_key_here
CHROMA_TENANT=your_chroma_tenant_id_here
CHROMA_DATABASE=articles

# Qdrant Cloud
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_URL=your_qdrant_cluster_url_here

# Pinecone
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here

# Weaviate Cloud
WEAVIATE_API_KEY=your_weaviate_api_key_here
WEAVIATE_URL=your_weaviate_cluster_url_here

# Milvus/Zilliz Cloud
ZILLIZ_API_KEY=your_zilliz_api_key_here
ZILLIZ_ENDPOINT=your_zilliz_endpoint_here
